        <street name="h" orientation="EW" dir="2way">
        	<point x="10" y="80"/>
        	<point x="600" y="80"/>
        </street>        
        

        <street name="1" orientation="NS" dir="2way">
        	<point x="15" y="5"/>
        	<point x="10" y="600"/>
        </street>
        <street name="2" orientation="NS" dir="2way">
        	<point x="20" y="5"/>
        	<point x="20" y="600"/>
        </street>
        <street name="3" orientation="NS" dir="2way">
        	<point x="30" y="5"/>
        	<point x="30" y="600"/>
        </street>
        <street name="4" orientation="NS" dir="2way">
        	<point x="40" y="5"/>
        	<point x="40" y="600"/>
        </street>                    

        <street name="5" orientation="NS" dir="2way">
